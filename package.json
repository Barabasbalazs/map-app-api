    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/env": "^4.3.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.26.0",
    "fluent-json-schema": "^4.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "pino-pretty": "^10.3.1"
